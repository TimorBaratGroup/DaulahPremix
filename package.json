{
  "name": "daulah-premix",
  "description": "Daulah Premix official website",
  "version": "1.0.0",
  "author": "Keng Sheng Lee <ks@myprimosolutions.com>",
  "bugs": {
    "url": "https://github.com/TimorBaratGroup/DaulahPremix/issues"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-source-contentful": "^2.2.5",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "lodash": "^4.17.11",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "sharp": "^0.30.5",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "contentful-import": "^7.4.3",
    "dotenv": "^8.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^2.20.8",
    "gh-pages": "^2.0.1",
    "inquirer": "^6.2.2",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "yargs-parser": "^13.1.2"
  },
  "homepage": "https://github.com/TimorBaratGroup/DaulahPremix",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimorBaratGroup/DaulahPremix.git"
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "env-cmd -f .env.development gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  }
}
